

function MovieCard({movie}) {
    return (
        <Grid item xs={6} s={4} md={4} key={movie.id} >
        <Paper>
            <h3>{movie.title}</h3>
            <img src={movie.poster} alt={movie.title} onClick={ () => handleDetails(movie)}/>
        </Paper>
        </Grid>
    )
}

export default MovieCard;